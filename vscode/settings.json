{
    "editor.rulers": [
        80,
        120
    ],
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 100000,
    "editor.tabSize": 4,
    "editor.detectIndentation": true,
    "editor.wordWrap": "off",
    "workbench.iconTheme": "vscode-icons",
    "editor.fontFamily": "'FiraCode Nerd Font'",
    "editor.fontLigatures": true,
    "auto-close-tag.SublimeText3Mode": true,
    "vim.camelCaseMotion.enable": true,
    "vim.foldfix": true,
    "vim.hlsearch": true,
    "vim.statusBarColorControl": false,
    "vim.textwidth": 120,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "B"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "E"
            ],
            "after": [
                "$"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "B"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "E"
            ],
            "after": [
                "$"
            ]
        }
    ],
    "editor.bracketPairColorization.enabled": true,
    "editor.formatOnSave": true,
    "editor.guides.indentation": true,
    "editor.guides.highlightActiveIndentation": true,
    "editor.guides.highlightActiveBracketPair": true,
    "editor.language.brackets": [
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ],
        [
            "{",
            "}"
        ]
    ],
    "editor.language.colorizedBracketPairs": [
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ],
        [
            "{",
            "}"
        ]
    ],
    "editor.tokenColorCustomizations": {
        "strings": "#FFFB00",
        "variables": "#F8F8F8",
        "comments": "#7E8E9E",
        "functions": "#4DD800",
        "keywords": "#E93EC1",
        "numbers": "#AA66FF",
        "types": "#00C2FF",
        "textMateRules": [],
    },
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "icon": "terminal-cmd"
        },
        "Git Bash": {
            "source": "Git Bash"
        },
        "Arch (WSL)": {
            "path": "C:\\WINDOWS\\System32\\wsl.exe",
            "args": [
                "-d",
                "Arch"
            ]
        }
    },
    "terminal.integrated.defaultProfile.windows": "Arch (WSL)",
    "workbench.colorCustomizations": {
        "statusBar.background": "#181818",
        "statusBar.foreground": "#cecece",
        "sideBar.background": "#181818",
        "sideBar.foreground": "#cecece",
        "panel.background": "#181818",
        "panelTitle.activeForeground": "#eee",
        "panelTitle.inactiveForeground": "#cecece",
        "panel.dropBackground": "#181818",
        "activityBar.background": "#181818",
        "activityBar.foreground": "#cecece",
        "activityBarBadge.background": "#cecece",
        "activityBarBadge.foreground": "#181818",
        "badge.background": "#cecece",
        "badge.foreground": "#181818",
        "dropdown.background": "#cecece",
        "dropdown.foreground": "#181818",
        "editor.background": "#181818",
        "editor.foreground": "#cecece",
        "menu.background": "#181818",
        "menu.foreground": "#cecece",
        "tab.activeBackground": "#181818",
        "tab.activeForeground": "#cecece",
        "tab.inactiveBackground": "#333",
        "tab.inactiveForeground": "#cecece",
        "titleBar.activeBackground": "#181818",
        "titleBar.activeForeground": "#cecece",
        "titleBar.inactiveBackground": "#333",
        "titleBar.inactiveForeground": "#cecece",
        "selection.background": "#888",
        "editor.selectionBackground": "#ff0000"
    },
    "auto-close-tag.activationOnLanguage": [
        "xml",
        "php",
        "blade",
        "ejs",
        "jinja",
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "plaintext",
        "markdown",
        "vue",
        "liquid",
        "erb",
        "lang-cfml",
        "cfml",
        "HTML (EEx)",
        "HTML (Eex)",
        "plist",
        "twig"
    ],
    "notebook.cellToolbarLocation": {
        "default": "left"
    },
    "auto-close-tag.disableOnLanguage": [],
    "git.closeDiffOnOperation": true,
    "gitlens.telemetry.enabled": false,
    "gitlens.advanced.messages": {
        "suppressCreatePullRequestPrompt": true
    },
    "markdown.occurrencesHighlight.enabled": true,
    "php.executablePath": "/usr/local/bin/php",
    "php.completion.parameters": "parameters",
    "php.highlight-todo.enable": false,
    "http.proxyStrictSSL": false,
    "http.proxySupport": "off",
    "shellcheck.executablePath": "/usr/local/bin/shellcheck",
    "bashIde.shellcheckPath": "/usr/local/bin/shellcheck",
    "gitlens.codeLens.scopes": [
        "document",
        "containers",
        "blocks"
    ],
}